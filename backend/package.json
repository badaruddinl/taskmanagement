  "description": "",
  "dependencies": {
    "@fastify/jwt": "^10.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.6.2",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.16.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.5.3"
  }
