{
  "name": "taskmanagement-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts --poll",
    "build": "tsc -p tsconfig.json",
    "start": "tsx dist/index.js",
    "migration:generate": "tsx src/utils/migrationGenerate.util.ts",
    "migration:run": "npx tsx node_modules/typeorm/cli.js migration:run -d src/connections/database.ts",
    "migration:revert": "npx tsx node_modules/typeorm/cli.js migration:revert -d src/connections/database.ts"
  },
